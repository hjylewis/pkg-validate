{
  "name": "pkg-validate",
  "version": "0.0.0",
  "description": "Validate packages in parallel",
  "bin": "index.js",
  "repository": "https://github.com/hjylewis/pkg-validate",
  "author": "hjylewis",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "prepush": "node index.js"
  },
  "lint-staged": {
    "*.js": ["prettier --write", "eslint --fix", "git add"],
    "*.{json,md}": ["prettier --write", "git add"]
  },
  "pkg-validate": ["eslint ."],
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "prettier": "1.11.1"
  },
  "dependencies": {
    "cosmiconfig": "^4.0.0",
    "execa": "^0.10.0",
    "listr": "^0.13.0",
    "merge-stream": "^1.0.1",
    "npm-which": "^3.0.1",
    "rxjs": "^5.5.7",
    "spawn-rx": "^2.0.12",
    "split": "^1.0.1",
    "stream-to-observable": "^0.2.0"
  }
}
